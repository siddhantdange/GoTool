{
	  "name": "GoTool",
	  "description": "quicker bookmarks!",
	  "omnibox": {
	    "keyword": "go"
    },
	  "icons": {
	    "16": "icon.png"
	  },
	  "background": {
	    "scripts": ["background.js"]
	  },
		"content_scripts": [
    {
      "matches": ["https://ajax.googleapis.com/*"],
      "js": ["jquery.js"]
    }
  	],
		"permissions": [
      "storage"
    ],
		"web_accessible_resources": [
    	"templates/*"
  	],
	  "version": "0.5",
	  "minimum_chrome_version": "9",
	  "manifest_version": 2
}
